{
  "name": "bump-release-pr-Action",
  "version": "0.0.5",
  "private": true,
  "description": "",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "run-s test:*",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major",
    "test:lint": "eslint '*/**/*.{js,ts,tsx}'",
    "test:unit": "jest",
    "docs": "typedoc",
    "commit": "git-cz",
    "commitlint": "commitlint",
    "all": "npm run build && npm run test:lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/metrisk/bump-release-pr.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Metrisk",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.4.0",
    "@actions/github": "5.0.0",
    "conventional-changelog": "3.1.24",
    "conventional-changelog-metrisk": "git+ssh://git@github.com/metrisk/conventional-changelog-metrisk.git",
    "execa": "5.1.1",
    "git-semver-tags": "4.1.1",
    "semver-regex": "4.0.0",
    "standard-version": "9.3.1",
    "through2": "4.0.2"
  },
  "devDependencies": {
    "@arkweid/lefthook": "0.7.6",
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@octokit/core": "3.5.1",
    "@octokit/types": "6.19.0",
    "@swc-node/jest": "1.3.1",
    "@types/conventional-changelog": "3.1.1",
    "@types/conventional-changelog-core": "4.1.2",
    "@types/conventional-commits-parser": "3.0.2",
    "@types/git-semver-tags": "4.1.1",
    "@types/jest": "26.0.24",
    "@types/node": "16.11.11",
    "@types/nodegit": "0.27.3",
    "@types/standard-version": "7.0.1",
    "@typescript-eslint/parser": "4.28.3",
    "@vercel/ncc": "0.28.6",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.30.0",
    "eslint-config-standard-with-typescript": "20.0.0",
    "eslint-plugin-github": "4.1.3",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jest": "24.3.6",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-tsdoc": "0.2.14",
    "jest": "27.0.6",
    "js-yaml": "4.1.0",
    "npm-run-all": "4.1.5",
    "typedoc": "0.21.4",
    "typedoc-plugin-markdown": "3.10.3",
    "typescript": "4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
